{
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "dest": "dist",
  "lib": {
    "entryFile": "./public_api.ts"
  },
  "allowedNonPeerDependencies": [
    "@angular/animations",
    "@angular/common",
    "@angular/compiler",
    "@angular/core",
    "@angular/forms",
    "@angular/platform-browser",
    "@angular/platform-browser-dynamic",
    "@angular/router",
    "piscium-theme",
    "rxjs",
    "tslib",
    "zone.js"
