    "./src/app/theme/default-theme/colors.scss",
    "./src/app/theme/default-theme/typography.scss",
    "./src/app/theme/default-theme/utility-classes.scss",
    "./src/app/theme/default-theme/mixins.scss",
    "./src/app/theme/default-theme/variables.scss",
    "./src/app/theme/default-theme/bootstrap-utilities.scss",
